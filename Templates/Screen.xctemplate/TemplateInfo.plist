			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_ScreenName___Builder</string>
			<key>Description</key>
			<string>Builder name</string>
			<key>Identifier</key>
			<string>builderName</string>
			<key>Name</key>
			<string>Builder:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>static</string>
		</dict>
	</array>
	<key>Summary</key>
	<string>This generates a new Screen module using VOF arc.</string>
</dict>
