	<key>Identifier</key>
	<string>ru.abd.mobile</string>
	<key>Kind</key>
	<string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>The name of the Screen to create</string>
			<key>Identifier</key>
			<string>ScreenName</string>
			<key>Name</key>
			<string>New ScreenName Name:</string>
			<key>NotPersisted</key>
			<true/>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_ScreenName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_ScreenName___ViewController</string>
			<key>Description</key>
			<string>View Controller name</string>
			<key>Identifier</key>
			<string>viewControllerName</string>
			<key>Name</key>
			<string>ViewController:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_ScreenName___Feature</string>
			<key>Description</key>
			<string>Feature ScreenName name</string>
			<key>Identifier</key>
			<string>featureScreenName</string>
			<key>Name</key>
			<string>feature:</string>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>static</string>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_ScreenName___Updater</string>
			<key>Description</key>
			<string>Updater name</string>
